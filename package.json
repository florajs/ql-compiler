  "scripts": {
    "cover": "npm run cover:clean && npm run cover:mocha && npm run cover:check",
    "cover:clean": "rimraf coverage",
    "cover:check": "node ./node_modules/istanbul/lib/cli check-coverage",
    "cover:mocha": "node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --recursive test/*",
    "lint": "eslint --ext .js lib test",
    "mocha": "mocha --recursive",
    "test": "npm run mocha --silent test",
    "posttest": "npm run --silent cover"
